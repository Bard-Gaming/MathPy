function main() {
    log("Hello world!");
}

log(main());  # prints "Hello world!" from call, then null
# since we are printing the output.

function certain_value(val) {
    var result = [];
    for (i in range(val - 5, val + 5)) {
        result.append(i);
    }
    return result
}

log(certain_value(3));